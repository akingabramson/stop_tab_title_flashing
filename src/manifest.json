{
  "manifest_version": 2,
  "name": "Stop Tab Title Flashing",
  "version": "0.0.1",
  "description": "Keep tab titles from flashing.",
  "icons": {
    "16": "icon_16.png",
    "48": "icon_48.png",
    "128": "icon.png"
  },

  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["stop_tab_flashing.js"]
  }]
}
